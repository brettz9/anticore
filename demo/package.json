{
  "private": true,
  "name": "hello-anticore-express",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "scripts": {
    "instrument": "nyc instrument public public_instrumented",
    "cypress": "PORT=56302 cypress open",
    "open": "PORT=56302 nyc --silent node app/server.js --open --coverage",
    "start": "node app/server.js"
  },
  "dependencies": {
    "@lcf.vs/dom-engine": "^2.3.4",
    "express": "^4.17.1",
    "multer": "^1.4.2"
  },
  "nyc": {
    "cache": false,
    "excludeAfterRemap": true,
    "reporter": [
      "text",
      "json-summary",
      "lcov"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/hello-express"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.5",
    "cypress": "^7.3.0",
    "nyc": "^15.1.0",
    "open": "^8.0.8"
  }
}
